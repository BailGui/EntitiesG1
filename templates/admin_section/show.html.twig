{% extends 'base.html.twig' %}

{% block title %}Section{% endblock %}

{% block body %}
<div class="container mt-5">
    <h1 class="text-center mb-5">Section</h1>

    <table class="table table-dark table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ section.id }}</td>
            </tr>
            <tr>
                <th>SectionTitle</th>
                <td>{{ section.sectionTitle }}</td>
            </tr>
            <tr>
                <th>SectionDescription</th>
                <td>{{ section.sectionDescription }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_section_index') }}">back to list</a>

    <a href="{{ path('app_admin_section_edit', {'id': section.id}) }}">edit</a>

    {{ include('admin_section/_delete_form.html.twig') }}
</div>
{% endblock %}
